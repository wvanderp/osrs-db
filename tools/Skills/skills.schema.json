{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "OSRS-DB/skills.schema.json",
    "title": "Skills",
    "description": "A schema for the generated Skills.json file: an array of skill names.",
    "type": "array",
    "items": {
        "type": "string",
        "enum": [
            "Agility",
            "Attack",
            "Construction",
            "Cooking",
            "Crafting",
            "Defence",
            "Farming",
            "Firemaking",
            "Fishing",
            "Fletching",
            "Herblore",
            "Hitpoints",
            "Hunter",
            "Magic",
            "Mining",
            "Prayer",
            "Ranged",
            "Runecrafting",
            "Sailing",
            "Slayer",
            "Smithing",
            "Strength",
            "Thieving",
            "Woodcutting"
        ]
    },
    "uniqueItems": true
}
