{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type"   : "array",
  "items"  : {
    "type"                : "object",
    "properties"          : {
      "id"            : {"type": "integer", "minimum": -9007199254740991, "maximum": 9007199254740991},
      "name"          : {"type": "string"},
      "examine"       : {
        "anyOf": [ {"type": "string"}, {"type": "null"} ]
      },
      "size"          : {
        "anyOf": [ {"type": "integer", "minimum": -9007199254740991, "maximum": 9007199254740991}, {"type": "null"} ]
      },
      "combatLevel"   : {
        "anyOf": [ {"type": "integer", "minimum": -9007199254740991, "maximum": 9007199254740991}, {"type": "null"} ]
      },
      "attackSpeed"   : {
        "anyOf": [ {"type": "integer", "minimum": -9007199254740991, "maximum": 9007199254740991}, {"type": "null"} ]
      },
      "aggressive"    : {
        "anyOf": [ {"type": "boolean"}, {"type": "null"} ]
      },
      "poisonous"     : {
        "anyOf": [ {"type": "boolean"}, {"type": "null"} ]
      },
      "immuneToPoison": {
        "anyOf": [ {"type": "boolean"}, {"type": "null"} ]
      },
      "immuneToVenom" : {
        "anyOf": [ {"type": "boolean"}, {"type": "null"} ]
      },
      "options"       : {
        "anyOf": [
          {
            "type" : "array",
            "items": {
              "anyOf": [ {"type": "string"}, {"type": "null"} ]
            }
          },
          {"type": "null"}
        ]
      },
      "hitpoints"     : {
        "anyOf": [ {"type": "integer", "minimum": -9007199254740991, "maximum": 9007199254740991}, {"type": "null"} ]
      },
      "attributes"    : {
        "anyOf": [
          { "type": "object", "propertyNames": {"type": "string"}, "additionalProperties": {} },
          { "type": "null"                                                                    }
        ]
      },
      "animations"    : {
        "anyOf": [
          { "type": "object", "propertyNames": {"type": "string"}, "additionalProperties": {} },
          { "type": "null"                                                                    }
        ]
      },
      "params"        : {
        "anyOf": [
          { "type": "object", "propertyNames": {"type": "string"}, "additionalProperties": {} },
          { "type": "null"                                                                    }
        ]
      }
    },
    "required"            : ["id", "name"],
    "additionalProperties": {}
  }
}
