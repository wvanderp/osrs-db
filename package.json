{
  "name": "osrs-db",
  "version": "1.6.2322",
  "description": "Machine-readable data for Old School RuneScape",
  "keywords": [
    "osrs",
    "oldschool",
    "runescape",
    "database",
    "items",
    "npcs",
    "objects",
    "quests"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wvanderp/osrs-db.git"
  },
  "files": [
    "data/**/*.json",
    "types/**/*.d.ts",
    "tools/**/*.schema.json",
    "*.mjs",
    "*.d.ts",
    "*.d.ts.map",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    "./data/*": "./data/*"
  },
  "typesVersions": {
    "*": {
      "data/*": [
        "types/index.d.ts"
      ]
    }
  },
  "sideEffects": false,
  "scripts": {
    "collect": "tsx collect/main.ts",
    "lint:data": "tsx collect/linters.ts",
    "lint": "tsc --noEmit -p tsconfig.json",
    "lint:dev": "tsx common/lintDev.ts",
    "update-version": "tsx scripts/update-version.ts",
    "generate-types": "tsx scripts/generate-types.ts",
    "build": "tsx scripts/build.ts"
  },
  "devDependencies": {
    "@types/jsdom": "^27.0.0",
    "@types/jsoneditor": "^9.9.6",
    "@types/node": "^24.10.1",
    "@types/yauzl": "^2.10.3",
    "ajv": "^8.17.1",
    "axios": "^1.13.2",
    "glob": "^13.0.0",
    "jsdom": "^27.2.0",
    "json-schema-to-typescript": "^15.0.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "yauzl": "^3.2.0"
  }
}
